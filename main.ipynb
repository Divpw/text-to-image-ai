    "# @title 3. Define Gradio User Interface\n",
    "\n",
    "status_textbox_ref = [None] \n",
    "\n",
    "def create_gradio_ui_colab():\n",
    "    global status_textbox_ref\n",
    "    dimension_choices = [\"512x512\", \"512x768\", \"768x512\", \"768x768\", \"1024x1024\", \"768x1024\", \"1024x768\"]\n",
